{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [
      "/*.{png,jpg,gif,ico,svg}",
      "/images/*.{png,jpg,gif,ico,svg}",
      "/css/*"
    ]
  },
  "routes": [   
    {
      "route": "/admin/*",
      "allowedRoles": [
        "admin"       
      ]
    },
    {
      "route": "/api/processhub/negotiate",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/api/Account/Status",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/api/*",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/app/*",
      "allowedRoles": [
        "authenticated"
      ]
    }
  ],
  "responseOverrides": {
  }
}

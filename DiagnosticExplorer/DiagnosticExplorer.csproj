<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>DiagnosticExplorer</AssemblyName>
    <RootNamespace>DiagnosticExplorer</RootNamespace>
    <Version>3.1.34</Version>
    <AssemblyVersion>3.1.34.0</AssemblyVersion>
    <FileVersion>3.1.34.0</FileVersion>
    <LangVersion>latest</LangVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  
  <ItemGroup>
    <Compile Remove="DiagnosticService.cs" />
    <Compile Remove="Util\AsyncResultBucket.cs" />
  </ItemGroup>
 
  <ItemGroup>
    <None Remove="DiagnosticExplorer.csproj.DotSettings" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="log4net" />
    <PackageReference Include="protobuf-net" />
    <PackageReference Include="System.Net.Http" />
    <PackageReference Include="System.Reactive" />
    <PackageReference Include="System.Text.Json" />
    <PackageReference Include="System.Threading.Channels" />
  </ItemGroup>

</Project>

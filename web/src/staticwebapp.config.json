{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/*.{png,jpg,gif,ico,svg}", "/images/*.{png,jpg,gif,ico,svg}", "/css/*"]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": [ "authenticated" ]
    },
     {
      "route": "/admin/*",
      "allowedRoles": ["Application Administrator"]
    }
  ],
  "responseOverrides": {

  }
}

